public interface IUserService {
	/*
	 * Method to create a new User
	 * @param Client data from User
	 * 
	 * */
	public void createUser(RpUser rpUser);
	
	/*
	 * Method to get a record from User
	 * @param int id
	 * 
	 * */
	public void getUserById(int id);

}
